{
    "name": "axstrad/axstrad",
    "type": "library",
    "description": "The Axstrad PHP Library develpoment bundle",
    "keywords": [],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "axstrad/common": "0.1.*",
        "axstrad/test": "0.1.*",
        "doctrine/common": "~2.3",
        "doctrine/doctrine-bundle": "~1",
        "doctrine/orm": "~2.3",
        "gedmo/doctrine-extensions": "~2.3",
        "phpoption/phpoption": "~1.3",
        "sensio/framework-extra-bundle": "~2.3",
        "sonata-project/admin-bundle": "~2.3",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.*",
        "sonata-project/seo-bundle": "~1.1",
        "stof/doctrine-extensions-bundle": "~1.1@dev",
        "symfony-cmf/seo-bundle": "~1",
        "symfony/symfony": "~2.3,<2.6",
        "twig/twig": "~1"
    },
    "require-dev": {
        "burgov/key-value-form-bundle": "~1.0",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "mikey179/vfsStream": "~1.4",
        "phpunit/phpunit": "~4.1",
        "symfony/monolog-bundle": "~2"
    },

    "autoload": {
        "psr-0": {
            "Axstrad\\": "src/"
        }
    },

    "archive": {
        "exclude": [
            "!vendor",
            "phpunit.xml*",
            "src/Axstrad/*/Tests",
            "src/Axstrad/Bundle/*/Tests",
            "src/Axstrad/DoctrineExtensions/*/Tests"
        ]
    },

    "provides": {
        "axstrad/test-bundle": "self.version"
    }
}
