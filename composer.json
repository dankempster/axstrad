{
    "name": "axstrad/axstrad",
    "type": "library",
    "description": "The Axstrad PHP Library",
    "keywords": [],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpoption/phpoption": "~1.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/http-foundation": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/options-resolver": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },

    "autoload": {
        "psr-0": {
            "Axstrad\\": "src/"
        }
    },

    "provide": {
        "axstrad/browser-sync-bundle": "self.version"
    },

    "archive": {
        "exclude": [
            "!vendor",
            "phpunit.xml*",
            "src/Axstrad/*/Tests",
            "src/Axstrad/Bundle/*/Tests"
        ]
    }
}
