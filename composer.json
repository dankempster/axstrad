{
    "name": "axstrad/axstrad",
    "type": "library",
    "description": "The Axstrad PHP Library",
    "keywords": [],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "homepage": "http://www.dankempster.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.3",
        "phpoption/phpoption": "~1.3",
        "sensio/framework-extra-bundle": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/http-foundation": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/options-resolver": "~2.3",
        "twig/twig": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "symfony/config": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/validator": "~2.3"
    },

    "autoload": {
        "psr-0": {
            "Axstrad\\": "src/"
        }
    },

    "archive": {
        "exclude": [
            "!vendor",
            "phpunit.xml*",
            "src/Axstrad/*/Tests"
        ]
    },

    "provides": {
        "axstrad/filesystem": "",
        "axstrad/extra-framework-bundle": ""
    }
}
