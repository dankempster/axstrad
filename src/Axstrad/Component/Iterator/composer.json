{
    "name": "axstrad/iterator",
    "description": "Additional iterators, mostly for iterating files like CSVs",
    "type": "library",
    "keywords": ["axstrad", "iterator", "csv", "file"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk"
        }
    ],

    "require": {
        "php": ">=5.2.2",
        "axstrad/common": "0.1.*"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\Component\\Iterator\\": ""
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
